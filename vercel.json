{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "src/index.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }